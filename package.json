{
  "name": "temp-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npm rebuild lightningcss --update-binary || true",
    "test": "vitest run",
    "test:ci": "vitest run --reporter=verbose",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "coverage:verify": "node -e \"const fs=require('fs');const cov=JSON.parse(fs.readFileSync('coverage/coverage-summary.json'));console.log('Coverage Summary:', cov.total);console.log('Files:', Object.keys(cov).filter(k=>k!=='total').length);\""
